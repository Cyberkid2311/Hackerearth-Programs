
#include <stdio.h>

int main()
{
	int i,j,t,n;
	scanf("%d",&t);
	while(t)
	{
	    scanf("%d",&n);
	    for(i=1;i<=n;i++)
	    {
	        for(j=1;j<=n*2;j++)
	        {
	          if(j<=i||j>=n*2+1-i)

	            printf("*");
	            else
	            printf("#");
	        }
	        printf("\n");
	    }

        printf("\n");
        t--;
    }
    

    
  }
  

